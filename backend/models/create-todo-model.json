{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "pattern": "^(?!\\s*$).+"
    },
    "desc": {
      "type": "string"
    },
    "dueDate": {
      "type": "string"
    }
  },
  "required": [
    "name",
    "desc",
    "dueDate"
  ],
  "additionalProperties": false
}
